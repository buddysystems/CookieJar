{
    "name": "Cookie Jar",
    "description": "Cookie Jar",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "cookies", "downloads"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "src/popup.html"
    },
    "icons": {
        "48": "src/assets/icons/icon48.png",
        "128": "src/assets/icons/icon128.png"
    }
}
